{
    "env": {
        "browser": true,
        "es2021": true,
       "jest": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "eslint:all", "plugin:react/recommended", "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-uses-react": "error",
        "react/jsx-uses-vars": "error",
        "react/react-in-jsx-scope": "off",
        "no-console": "off",
        "no-eval": "off",
        "import/first": 0,
        "interface-name": 0,
        "jsx-no-lambda": 0
    }
}
